{
  "manifest_version": 2,
  "version": "0.1.0",
  "name": "linkhere",
  "description": "Shows you webpages saved from your mobile device in new tabs.",
  "author": "Max Isom",
  "chrome_url_overrides": {
    "newtab": "app.html"
  },
  "permissions": ["storage"],
  "web_accessible_resources": ["resources/*"],
  "icons": {
    "16": "resources/logo/16px.png",
    "48": "resources/logo/48px.png",
    "128": "resources/logo/128px.png"
  }
}
